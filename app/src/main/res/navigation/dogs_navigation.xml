<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/dogs_navigation"
    app:startDestination="@id/listFragment">

    <fragment
        android:id="@+id/listFragment"
        android:name="com.example.jetpack.view.ListFragment"
        android:label="ListFragment" >
        <action
            android:id="@+id/action_listFragment_to_detailFrgament"
            app:destination="@id/detailFrgament" />
    </fragment>
    <fragment
        android:id="@+id/detailFrgament"
        android:name="com.example.jetpack.view.DetailFrgament"
        android:label="DetailFrgament" >
        <action
            android:id="@+id/action_detailFrgament_to_listFragment"
            app:destination="@id/listFragment" />
    </fragment>
</navigation>